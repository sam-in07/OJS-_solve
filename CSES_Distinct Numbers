 const long long MOD = 1000000007;

int primeFactors(int n){
    while(n%2==0){
        return 2;
    }
    for (int i=3;i<=sqrt(n);i=i+2){
        while (n%i==0){
            return i;
        }
    } 
    if(n>2){
        return n;
    }
    return -1;
}
